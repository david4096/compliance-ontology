#################################################################
#    General Axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Attestation
                :AttestationType
                :Attester
                :Baseline
                :ComplianceFramework
                :ComplianceStatus
                :System
              )
] .
