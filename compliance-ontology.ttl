@prefix : <http://example.org/compliance#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://example.org/compliance#> .

<http://example.org/compliance> rdf:type owl:Ontology ;
    dcterms:title "Compliance Framework Ontology" ;
    dcterms:description "An ontology for representing compliance frameworks, attestations, and compliance status" ;
    dcterms:created "2025-10-08"^^xsd:date ;
    owl:versionInfo "1.0.0" .

#################################################################
#    Object Properties
#################################################################

:hasAttestation rdf:type owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Attestation ;
    rdfs:comment "Links a system to its compliance attestation" ;
    rdfs:label "has attestation" .

:attestsCompliance rdf:type owl:ObjectProperty ;
    rdfs:domain :Attestation ;
    rdfs:range :ComplianceFramework ;
    rdfs:comment "Links an attestation to the framework it attests compliance with" ;
    rdfs:label "attests compliance" .

:hasComplianceStatus rdf:type owl:ObjectProperty ;
    rdfs:domain :Attestation ;
    rdfs:range :ComplianceStatus ;
    rdfs:comment "The compliance status of an attestation" ;
    rdfs:label "has compliance status" .

:hasAttestationType rdf:type owl:ObjectProperty ;
    rdfs:domain :Attestation ;
    rdfs:range :AttestationType ;
    rdfs:comment "The type of attestation (self-attested, third-party, etc.)" ;
    rdfs:label "has attestation type" .

:attestedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :Attestation ;
    rdfs:range :Attester ;
    rdfs:comment "The entity that provided the attestation" ;
    rdfs:label "attested by" .

:hasBaseline rdf:type owl:ObjectProperty ;
    rdfs:domain :ComplianceFramework ;
    rdfs:range :Baseline ;
    rdfs:comment "Links a framework to its baselines (e.g., FedRAMP Low/Moderate/High)" ;
    rdfs:label "has baseline" .

#################################################################
#    Data Properties
#################################################################

:frameworkVersion rdf:type owl:DatatypeProperty ;
    rdfs:domain :ComplianceFramework ;
    rdfs:range xsd:string ;
    rdfs:comment "The version of the compliance framework" ;
    rdfs:label "framework version" .

:documentationURL rdf:type owl:DatatypeProperty ;
    rdfs:domain :ComplianceFramework ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "URL to official documentation for the framework" ;
    rdfs:label "documentation URL" .

:homepageURL rdf:type owl:DatatypeProperty ;
    rdfs:domain :ComplianceFramework ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "URL to the framework's main homepage or information page" ;
    rdfs:label "homepage URL" .

:publicationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :ComplianceFramework ;
    rdfs:range xsd:date ;
    rdfs:comment "Date the framework version was published" ;
    rdfs:label "publication date" .

:attestationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Attestation ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Date and time the attestation was made" ;
    rdfs:label "attestation date" .

:expirationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Attestation ;
    rdfs:range xsd:date ;
    rdfs:comment "Date when the attestation expires" ;
    rdfs:label "expiration date" .

:attestationEvidence rdf:type owl:DatatypeProperty ;
    rdfs:domain :Attestation ;
    rdfs:range xsd:string ;
    rdfs:comment "Supporting evidence or documentation for the attestation" ;
    rdfs:label "attestation evidence" .

:attestedFrameworkVersion rdf:type owl:DatatypeProperty ;
    rdfs:domain :Attestation ;
    rdfs:range xsd:string ;
    rdfs:comment "The specific version of the framework being attested to" ;
    rdfs:label "attested framework version" .

:attesterName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Attester ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the attesting entity" ;
    rdfs:label "attester name" .

:attesterCredentials rdf:type owl:DatatypeProperty ;
    rdfs:domain :Attester ;
    rdfs:range xsd:string ;
    rdfs:comment "Credentials or certifications of the attester" ;
    rdfs:label "attester credentials" .

:systemName rdf:type owl:DatatypeProperty ;
    rdfs:domain :System ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the system being attested" ;
    rdfs:label "system name" .

:systemDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain :System ;
    rdfs:range xsd:string ;
    rdfs:comment "Description of the system" ;
    rdfs:label "system description" .

:baselineName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Baseline ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the baseline (e.g., 'Low', 'Moderate', 'High')" ;
    rdfs:label "baseline name" .

:controlCount rdf:type owl:DatatypeProperty ;
    rdfs:domain :Baseline ;
    rdfs:range xsd:integer ;
    rdfs:comment "Number of controls in this baseline" ;
    rdfs:label "control count" .

#################################################################
#    Classes
#################################################################

:System rdf:type owl:Class ;
    rdfs:comment "A system, application, or organization that may have compliance attestations" ;
    rdfs:label "System" .

:Attestation rdf:type owl:Class ;
    rdfs:comment "A compliance attestation for a system" ;
    rdfs:label "Attestation" .

:ComplianceFramework rdf:type owl:Class ;
    rdfs:comment "A compliance or security framework" ;
    rdfs:label "Compliance Framework" .

:Baseline rdf:type owl:Class ;
    rdfs:comment "A specific baseline within a framework (e.g., FedRAMP Moderate)" ;
    rdfs:label "Baseline" .

:Attester rdf:type owl:Class ;
    rdfs:comment "An entity that provides attestation (individual, organization, third-party auditor)" ;
    rdfs:label "Attester" .

:ComplianceStatus rdf:type owl:Class ;
    rdfs:comment "The status of compliance" ;
    rdfs:label "Compliance Status" .

:AttestationType rdf:type owl:Class ;
    rdfs:comment "The type of attestation" ;
    rdfs:label "Attestation Type" .

#################################################################
#    Individuals - Compliance Status
#################################################################

:Compliant rdf:type owl:NamedIndividual ,
    :ComplianceStatus ;
    rdfs:comment "Fully compliant with the framework" ;
    rdfs:label "Compliant" .

:InProgress rdf:type owl:NamedIndividual ,
    :ComplianceStatus ;
    rdfs:comment "Working towards compliance" ;
    rdfs:label "In Progress" .

:NonCompliant rdf:type owl:NamedIndividual ,
    :ComplianceStatus ;
    rdfs:comment "Not compliant with the framework" ;
    rdfs:label "Non-Compliant" .

:PartiallyCompliant rdf:type owl:NamedIndividual ,
    :ComplianceStatus ;
    rdfs:comment "Partially compliant with the framework" ;
    rdfs:label "Partially Compliant" .

#################################################################
#    Individuals - Attestation Types
#################################################################

:SelfAttested rdf:type owl:NamedIndividual ,
    :AttestationType ;
    rdfs:comment "Attestation provided by the organization itself" ;
    rdfs:label "Self-Attested" .

:ThirdPartyAttested rdf:type owl:NamedIndividual ,
    :AttestationType ;
    rdfs:comment "Attestation provided by an independent third party" ;
    rdfs:label "Third-Party Attested" .

:CertifiedCompliant rdf:type owl:NamedIndividual ,
    :AttestationType ;
    rdfs:comment "Officially certified as compliant by an authorized body" ;
    rdfs:label "Certified Compliant" .

:AuditorVerified rdf:type owl:NamedIndividual ,
    :AttestationType ;
    rdfs:comment "Verified by an independent auditor" ;
    rdfs:label "Auditor Verified" .

